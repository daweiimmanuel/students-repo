<% include ../partials/header %>
    <div class="ui main container segment">
        <h1 class="ui center aligned header">Counsellors</h1>
        <div class="ui cards">
            <%counsellors.forEach(function(counsellor) { %>
                <div class="card">
                    <div class="content">
                        <a href="<%= counsellor.url %>">
                            <div class="header">
                                <%= counsellor.counsellorName%>
                            </div>
                        </a>
                    </div>
                </div>
            <% })%>
        </div>
        <br>
        <div class="ui container">
            <a href="/catalog/counsellor/create">
                <button class="ui primary button" type="submit">Add New Counsellor</button>
            </a>
        </div>
    </div>

<% include ../partials/footer %>