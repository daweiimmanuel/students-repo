<% include ../partials/header %>
    <div class="ui main container segment">
        <h1 class="ui center aligned header">Events</h1>
        <div class="ui cards">
            <%events.forEach(function(event) { %>
                <div class="card">
                    <div class="content">
                        <a href="<%= event.url %>">
                            <div class="header">
                                <%= event.eventName%>
                            </div>
                        </a>
                    </div>
                </div>
            <% })%>
        </div>
        <br>
        <div class="ui container">
            <a href="/catalog/event/create">
                <button class="ui primary button" type="submit">Add New Event</button>
            </a>
        </div>
    </div>

<% include ../partials/footer %>