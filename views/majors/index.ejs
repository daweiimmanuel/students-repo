<% include ../partials/header %>
    <div class="ui main container segment">
        <h1 class="ui center aligned header">Majors</h1>
        <div class="ui cards">
            <%majors.forEach(function(major) { %>
                <div class="card">
                    <div class="content">
                        <a href="<%= major.url %>">
                            <div class="header">
                                <%= major.majorName%>
                            </div>
                        </a>
                    </div>
                </div>
            <% })%>
        </div>
        <br>
        <div class="ui container">
            <a href="/catalog/major/create">
                <button class="ui primary button" type="submit">Add New Major</button>
            </a>
        </div>
    </div>

<% include ../partials/footer %>